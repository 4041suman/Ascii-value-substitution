class HelloWorld {
    public static void main(String[] args) {
        char ch1='A';
        char ch2='b';
        int asciivalue1=ch1;
        int asciivalue2=ch2;
        System.out.println("ASCII value is "+ch1+" :" +asciivalue1);
        System.out.println("ASCII value is "+ch2+" :" +asciivalue2);
    }
}
